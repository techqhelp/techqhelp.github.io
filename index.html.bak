        const itemsPerPage = 5;
        let currentPage = 0;
        const totalPages = Math.ceil(testimonials.length / itemsPerPage);
        
        function renderTestimonials(page) {
            const container = document.getElementById('testimonial-cards-container');
            container.innerHTML = '';
            
            const start = page * itemsPerPage;
            const end = Math.min(start + itemsPerPage, testimonials.length);
            
            for (let i = start; i < end; i++) {
                const t = testimonials[i];
                const col = document.createElement('div');
                col.className = 'col-lg-2 col-md-4 col-sm-6 mb-3';
                col.innerHTML = `
                    <div class="testimonial-card-grid shadow-sm rounded-lg p-2 h-100 position-relative" style="background:#fff; border-radius:12px; border:1px solid #eee; min-width:160px; margin:8px; display:flex; flex-direction:column; align-items:center; transition:transform 0.3s;">
                        <div style="height:4px; background:#4bc714; border-radius:12px 12px 0 0; position:absolute; top:0; left:0; right:0;"></div>
                        <img src='${t.img}' alt='${t.name}' style="width:48px; height:48px; border-radius:50%; box-shadow:0 2px 8px rgba(76,199,20,0.13); border:3px solid #fff; margin-top:1.2rem; margin-bottom:0.6rem;" />
                        <div style="font-weight:700; font-size:0.95rem; color:#052501; text-align:center;">${t.name}</div>
                        <div style="color:#4bc714; font-size:0.85rem; font-weight:600; text-align:center; margin-bottom:0.6rem;">${t.role}</div>
                        <div class="testimonial-quote-grid mb-2" style="font-size:0.85rem; color:#222; font-style:italic; text-align:center; background:#eafbe7; border-radius:10px; padding:0.8rem 0.6rem; margin-top:0.2rem; line-height:1.4;">${t.quote}</div>
                    </div>
                `;
                container.appendChild(col);
            }
            
            // Update dots
            const dotsContainer = document.getElementById('testimonial-dots');
            dotsContainer.innerHTML = '';
            for (let i = 0; i < totalPages; i++) {
                const dot = document.createElement('span');
                dot.style.width = '12px';
                dot.style.height = '12px';
                dot.style.borderRadius = '50%';
                dot.style.display = 'inline-block';
                dot.style.background = i === page ? '#4bc714' : '#eafbe7';
                dot.style.cursor = 'pointer';
                dot.style.transition = 'all 0.3s';
                dot.onclick = () => {
                    currentPage = i;
                    renderTestimonials(i);
                };
                dotsContainer.appendChild(dot);
            }
        }
        
        document.getElementById('testimonial-prev').onclick = function() {
            currentPage = (currentPage - 1 + totalPages) % totalPages;
            renderTestimonials(currentPage);
        };
        
        document.getElementById('testimonial-next').onclick = function() {
            currentPage = (currentPage + 1) % totalPages;
            renderTestimonials(currentPage);
        };
        
        // Initial render
        renderTestimonials(0);
        </script>
  
