        <script>
            (function(){
                // Language suggestion banner: detect browser language and offer switch.
                var key = 'myhal_lang_suggest_dismissed_v1';
                try{
                    if(localStorage.getItem(key)) return;
                }catch(e){/* localStorage not available */}

                function isOnHiPage(){
                    var p = location.pathname.replace(/\/$/, '');
                    return /(^|\/)hi(\/|$)/i.test(p) || /hi\/index\.html$/i.test(p);
                }

                var lang = (navigator.language || navigator.userLanguage || '').toLowerCase();
                // small delay so the page appears smoother
                setTimeout(function(){
                    var wantHi = /^hi/i.test(lang);
                    var onHi = isOnHiPage();
                    if(wantHi && !onHi){
                        showBanner('hi');
                    } else if(!wantHi && onHi){
                        showBanner('en');
                    }
                }, 600);

                function showBanner(target){
                    var b = document.getElementById('langBanner');
                    if(!b) return;
                    b.style.display = 'block';
                    document.body.classList.add('lang-banner-visible');
                    var switchBtn = document.getElementById('langBannerSwitch');
                    var closeBtn = document.getElementById('langBannerClose');
                    if(switchBtn){
                        if(target === 'hi'){
                            switchBtn.setAttribute('href','hi/index.html');
                            switchBtn.textContent = 'Switch to ‡§π‡§ø‡§®‡•ç‡§¶‡•Ä';
                        } else {
                            switchBtn.setAttribute('href','index.html');
                            switchBtn.textContent = 'Switch to English';
                        }
                    }
                    if(closeBtn){
                        closeBtn.addEventListener('click', function(ev){
                            ev.preventDefault();
                            try{ localStorage.setItem(key,'1'); }catch(e){}
                            b.style.display = 'none';
                            document.body.classList.remove('lang-banner-visible');
                        }, {once:true});
                    }
                }
            })();
        </script>
                <script>
                    // Ensure the right-side hero mockup matches the left column height on large screens.
                    (function(){
                        function debounce(fn, wait){
                            var t; return function(){ clearTimeout(t); t = setTimeout(fn, wait); };
                        }
                                    function syncHeroVisualHeight(){
                                        try{
                                            var leftCol = document.querySelector('.hero .container .row > .col-lg-6:not(.hero-visual)');
                                            var visualCard = document.querySelector('.hero-visual .visual-card');
                                            var img = visualCard ? visualCard.querySelector('img') : null;
                                            if(!leftCol || !visualCard) return;
                                            var leftH = Math.round(leftCol.getBoundingClientRect().height);
                                            // cap the visual card to the left column height
                                            visualCard.style.maxHeight = leftH + 'px';
                                            // set image max-height to a percentage of left height (85%) so person+phone fit comfortably
                                            if(img){ img.style.maxHeight = Math.round(leftH * 0.85) + 'px'; img.style.height = 'auto'; }
                                        }catch(e){/* ignore */}
                                    }
                        window.addEventListener('load', syncHeroVisualHeight);
                        window.addEventListener('resize', debounce(syncHeroVisualHeight, 120));
                        // re-run after fonts/images may load
                        setTimeout(syncHeroVisualHeight, 600);
                    })();
                </script>
