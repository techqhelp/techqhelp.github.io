            {img:'images/user5.png', name:'Murugesan', role:'Salem, Tamil Nadu', quote:'"I sell vegetables directly without middlemen. Better rates, smooth process."'},
            {img:'images/user6.png', name:'Biju Thomas', role:'Kottayam, Kerala', quote:'"Posting ads is easy and buyers contact me quickly. Highly recommended."'},
            {img:'images/user7.png', name:'Ramilaben Patel', role:'Anand, Gujarat', quote:'"Dairy products reach customers directly with fair prices and quick payments."'},
            {img:'images/user8.png', name:'Ganesh Patil', role:'Sangli, Maharashtra', quote:'"Farm tools and crops sold at fair prices. The app is simple and reliable."'}  
        ];
        
        let itemsPerPage = 4;
        let currentPage = 0;
        let totalPages = Math.ceil(testimonials.length / itemsPerPage);
        
        function calculateItemsPerPage() {
            const width = window.innerWidth;
            if (width >= 1200) return 4;      // Large screens: 4 items (col-lg-3)
            else if (width >= 768) return 2;  // Medium screens: 2 items (col-md-6)
            else return 1;                    // Small screens: 1 item (col-sm-12)
        }
        
        function renderTestimonials(page) {
            itemsPerPage = calculateItemsPerPage();
            totalPages = Math.ceil(testimonials.length / itemsPerPage);
            
            // Adjust current page if it's out of bounds
            if (currentPage >= totalPages) {
                currentPage = totalPages - 1;
            }
            
            const container = document.getElementById('testimonial-cards-container');
            container.innerHTML = '';
            
            const start = page * itemsPerPage;
            const end = Math.min(start + itemsPerPage, testimonials.length);
            
            for (let i = start; i < end; i++) {
                const t = testimonials[i];
                const col = document.createElement('div');
                col.className = 'col-lg-3 col-md-6 col-sm-12 mb-4';
                col.innerHTML = `
                    <div class="testimonial-card-modern shadow-sm h-100" style="background:#fff; border-radius:16px; border-top:4px solid #4bc714; padding:2rem 1.5rem; display:flex; flex-direction:column; transition:all 0.3s ease; min-height:280px;">
                        <div style="font-size:0.95rem; color:#666; font-style:italic; text-align:left; line-height:1.6; margin-bottom:1.5rem; flex-grow:1;">${t.quote}</div>
                        <div style="display:flex; align-items:center; gap:1rem;">
                            <img src='${t.img}' alt='${t.name}' style="width:60px; height:60px; border-radius:50%; object-fit:cover; border:3px solid #4bc714; flex-shrink:0;" />
                            <div style="text-align:left;">
                                <div style="font-weight:700; font-size:1rem; color:#052501; margin-bottom:0.2rem;">${t.name}</div>
                                <div style="color:#999; font-size:0.85rem;">${t.role}</div>
                            </div>
                        </div>
                    </div>
                `;
                container.appendChild(col);
            }
            
            // Update dots
            const dotsContainer = document.getElementById('testimonial-dots');
            dotsContainer.innerHTML = '';
            for (let i = 0; i < totalPages; i++) {
                const dot = document.createElement('span');
                dot.style.width = '12px';
                dot.style.height = '12px';
                dot.style.borderRadius = '50%';
                dot.style.display = 'inline-block';
                dot.style.background = i === page ? '#4bc714' : '#eafbe7';
                dot.style.cursor = 'pointer';
                dot.style.transition = 'all 0.3s';
                dot.onclick = () => {
                    currentPage = i;
                    renderTestimonials(i);
                };
                dotsContainer.appendChild(dot);
            }
        }
        
        document.getElementById('testimonial-prev').onclick = function() {
            currentPage = (currentPage - 1 + totalPages) % totalPages;
            renderTestimonials(currentPage);
        };
        
        document.getElementById('testimonial-next').onclick = function() {
            currentPage = (currentPage + 1) % totalPages;
            renderTestimonials(currentPage);
        };
        
        // Initial render
        renderTestimonials(0);
        
        // Handle window resize
        let resizeTimer;
        window.addEventListener('resize', function() {
            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function() {
                renderTestimonials(currentPage);
            }, 250);
        });
        
        // Animated Counter for Stats
        function animateCounter(element, target, duration = 2000) {
            let start = 0;
            const suffix = element.getAttribute('data-suffix') || '';
            const increment = target / (duration / 16);
            const timer = setInterval(() => {
                start += increment;
                if (start >= target) {
                    element.textContent = target.toLocaleString() + suffix;
                    clearInterval(timer);
                } else {
                    element.textContent = Math.floor(start).toLocaleString() + suffix;
                }
            }, 16);
        }
        
        // Intersection Observer for stats animation
        const statsObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting && !entry.target.classList.contains('counted')) {
                    entry.target.classList.add('counted');
                    const target = parseInt(entry.target.getAttribute('data-target'));
                    animateCounter(entry.target, target);
                }
            });
        }, { threshold: 0.3 });
        
        // Observe all stat numbers
        document.querySelectorAll('.stat-number').forEach(stat => {
            statsObserver.observe(stat);
        });
        </script>
  
